
funcdef ten_to_hundred :: One → Hundred
funcdef ten_to_hundred a
    return(a * 99)
suchthat:
    |    doc as "Takes a One and makes a 100"
end

funcdef one :: Numerical → One
funcdef one num
    return(1)
suchthat:
    |    doc as "Makes a One"
end

typedef One a :: Integer {a == 1}
suchthat:
    |   doc as "Is one!"
end

typedef Hundred a :: Integer {a < 100}
suchthat:
    |   doc as "Less than or equal to 100"
end
